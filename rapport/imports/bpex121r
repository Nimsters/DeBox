%abbrev
ex121r: {p}{q}{r}
    proof(p => (q /\ r) , |- 
         (p => q) /\ (p => r)) =
[p][q][r]
p => (q /\ r) premise; [@prem1]
( p assumption; [@p]
  (q /\ r) by imp_e @p @prem1 ; [@qor]
  q by con_e1 @qor
) ; [#qbox]
p => q by imp_i #qbox ; [@piq]
( p assumption; [@p1]
  (q /\ r) by imp_e @p1 @prem1 ; [@qor1]
  r by con_e2 @qor1
) ; [#rbox]
p => r by imp_i #rbox ; [@pir]
(p => q) /\ (p => r) by con_i @piq @pir
.
